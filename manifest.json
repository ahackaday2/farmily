{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "options_page": "options.html",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/whee.png"
        }
    },
    "icons": {
        "16": "/images/whee.png"
    }
}